enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-remi
exclude=php-* mysql* phpMyAdmin* roundcubemail* redis* wordpress* memcached* glpi* ocsinventory* fusioninventory* owncloud*
